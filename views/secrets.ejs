<%- include("partials/header") -%>

<div class="jumbotron text-center">
  <div class="container">
    <i class="fas fa-key fa-6x"></i>
    <h1 class="display-3">This is A lot of Secrets!</h1>

     <% usersWithSecrets.forEach(function(user) { %>  
        <p class="secret-text">
          <%= user.secrets %>
        </p>
      <%}) %>
   
    <hr>
    
    <a class="btn btn-light btn-lg" href="/" role="button">Log Out</a>
    <a class="btn btn-dark btn-lg" href="/submit" role="button">Submit a Secret</a>
  </div>
</div>

<%- include("partials/footer") -%>
